                                let apps2 = [
                                    {src: './assets/imgs/appstore/blue-prism.svg',alt: 'icon 1', h5: 'RoboManager for Blue Prism', p: 'Deliver superior service to your customers while cutting operational costs and reducing regulatory risk.',href: '#'},
                                    {src: './assets/imgs/appstore/uipath.svg',alt: 'icon 2', h5: 'RoboManager for UiPath', p: 'Deliver superior service to your customers while cutting operational costs and reducing regulatory risk.',href: '#'},
                                    {src: './assets/imgs/appstore/microsoft.svg',alt: 'icon 3', h5: 'RoboManager for Microsoft', p: 'Deliver superior service to your customers while cutting operational costs and reducing regulatory risk.',href: '#'},
                                    {src: './assets/imgs/appstore/auto.svg',alt: 'icon 4', h5: 'RoboManager for Automation Anywhere', p: 'Deliver superior service to your customers while cutting operational costs and reducing regulatory risk.',href: '#'},
                                ]
                            for app in apps2
                                li

                                    img(src=app.src, alt=app.alt)
                                    h5=app.h5
                                    p=app.p
                                    .appstore-btn
                                        a(href=app.href).btn Free Trial
                                        a(href=app.href).btn Download Now
            .appstore-application
                div
                    h4 Automation Products (Venfi Connectors BP, UiPath)
                    p Deliver superior service to your customers while cutting operational costs and reducing regulatory risk through Intelligent Automation solutions. Deliver superior service to your customers while cutting operational costs and reducing regulatory risk through Intelligent Automation solutions.
                    .item
